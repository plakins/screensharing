{
  "name": "Screensharing",
  "description": "Screensharing utility for tab capture",
  "version": "0.0.1",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "icons": {
  },
  "permissions": [
    "desktopCapture",
    "tabCapture",
    "activeTab"
  ],
  "browser_action": {
    "default_title": "Screensharing"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [ {
    "js": [ "content.js" ],
    "matches": [ "<all_urls>" ]
  }],
  "externally_connectable": {
      "matches": [
          "https://127.0.0.1/*"
      ],
      "ids": ["*"]
  }
}
